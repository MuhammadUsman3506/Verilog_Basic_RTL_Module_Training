:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe9274380 .scope module, "tb_mux_4_to_1" "tb_mux_4_to_1" 2 1;
 .timescale 0 0;
P_0x7fffe9274510 .param/l "WIDTH" 0 2 2, +C4<00000000000000000000000000100000>;
v0x7fffe928ac70_0 .var "i_data0", 31 0;
v0x7fffe928ad30_0 .var "i_data1", 31 0;
v0x7fffe928ae00_0 .var "i_data2", 31 0;
v0x7fffe928af00_0 .var "i_data3", 31 0;
v0x7fffe928afd0_0 .var "i_sel", 1 0;
v0x7fffe928b0c0_0 .net "o_data", 31 0, v0x7fffe928a8d0_0;  1 drivers
S_0x7fffe92745b0 .scope module, "inst_mux_4_to_1" "mux_4_to_1" 2 16, 3 1 0, S_0x7fffe9274380;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "i_data0";
    .port_info 1 /INPUT 32 "i_data1";
    .port_info 2 /INPUT 32 "i_data2";
    .port_info 3 /INPUT 32 "i_data3";
    .port_info 4 /INPUT 2 "i_sel";
    .port_info 5 /OUTPUT 32 "o_data";
P_0x7fffe9274880 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000100000>;
v0x7fffe9261530_0 .net "i_data0", 31 0, v0x7fffe928ac70_0;  1 drivers
v0x7fffe928a4f0_0 .net "i_data1", 31 0, v0x7fffe928ad30_0;  1 drivers
v0x7fffe928a5d0_0 .net "i_data2", 31 0, v0x7fffe928ae00_0;  1 drivers
v0x7fffe928a6c0_0 .net "i_data3", 31 0, v0x7fffe928af00_0;  1 drivers
v0x7fffe928a7a0_0 .net "i_sel", 1 0, v0x7fffe928afd0_0;  1 drivers
v0x7fffe928a8d0_0 .var "o_data", 31 0;
E_0x7fffe9272d20/0 .event edge, v0x7fffe928a7a0_0, v0x7fffe9261530_0, v0x7fffe928a4f0_0, v0x7fffe928a5d0_0;
E_0x7fffe9272d20/1 .event edge, v0x7fffe928a6c0_0;
E_0x7fffe9272d20 .event/or E_0x7fffe9272d20/0, E_0x7fffe9272d20/1;
S_0x7fffe928aa70 .scope task, "stimulus" "stimulus" 2 37, 2 37 0, S_0x7fffe9274380;
 .timescale 0 0;
TD_tb_mux_4_to_1.stimulus ;
    %vpi_func 2 39 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %store/vec4 v0x7fffe928ac70_0, 0, 32;
    %vpi_func 2 40 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %store/vec4 v0x7fffe928ad30_0, 0, 32;
    %vpi_func 2 41 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %store/vec4 v0x7fffe928ae00_0, 0, 32;
    %vpi_func 2 42 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %store/vec4 v0x7fffe928af00_0, 0, 32;
    %vpi_func 2 43 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000000011 {0 0 0};
    %pad/u 2;
    %store/vec4 v0x7fffe928afd0_0, 0, 2;
    %delay 5, 0;
    %end;
    .scope S_0x7fffe92745b0;
T_1 ;
    %wait E_0x7fffe9272d20;
    %load/vec4 v0x7fffe928a7a0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %jmp T_1.4;
T_1.0 ;
    %load/vec4 v0x7fffe9261530_0;
    %store/vec4 v0x7fffe928a8d0_0, 0, 32;
    %jmp T_1.4;
T_1.1 ;
    %load/vec4 v0x7fffe928a4f0_0;
    %store/vec4 v0x7fffe928a8d0_0, 0, 32;
    %jmp T_1.4;
T_1.2 ;
    %load/vec4 v0x7fffe928a5d0_0;
    %store/vec4 v0x7fffe928a8d0_0, 0, 32;
    %jmp T_1.4;
T_1.3 ;
    %load/vec4 v0x7fffe928a6c0_0;
    %store/vec4 v0x7fffe928a8d0_0, 0, 32;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x7fffe9274380;
T_2 ;
    %vpi_call 2 28 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffe9274380 {0 0 0};
    %pushi/vec4 10, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %fork TD_tb_mux_4_to_1.stimulus, S_0x7fffe928aa70;
    %join;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %end;
    .thread T_2;
    .scope S_0x7fffe9274380;
T_3 ;
    %delay 500, 0;
    %vpi_call 2 48 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
