#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd18beb60 .scope module, "tb_sub" "tb_sub" 2 1;
 .timescale 0 0;
P_0x7fffd18becf0 .param/l "WIDTH" 0 2 2, +C4<00000000000000000000000000100000>;
v0x7fffd18d2760_0 .var "in1", 31 0;
v0x7fffd18d2850_0 .var "in2", 31 0;
v0x7fffd18d2920_0 .net "out", 31 0, v0x7fffd18d2410_0;  1 drivers
S_0x7fffd18bed90 .scope module, "inst_sub" "sub" 2 11, 3 1 0, S_0x7fffd18beb60;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "in1";
    .port_info 1 /INPUT 32 "in2";
    .port_info 2 /OUTPUT 32 "out";
P_0x7fffd18bef70 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000100000>;
v0x7fffd18abd20_0 .net "in1", 31 0, v0x7fffd18d2760_0;  1 drivers
v0x7fffd18d2330_0 .net "in2", 31 0, v0x7fffd18d2850_0;  1 drivers
v0x7fffd18d2410_0 .var "out", 31 0;
E_0x7fffd18bd440 .event edge, v0x7fffd18abd20_0, v0x7fffd18d2330_0;
S_0x7fffd18d2580 .scope task, "stimulus" "stimulus" 2 32, 2 32 0, S_0x7fffd18beb60;
 .timescale 0 0;
TD_tb_sub.stimulus ;
    %vpi_func 2 34 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %store/vec4 v0x7fffd18d2760_0, 0, 32;
    %vpi_func 2 35 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %store/vec4 v0x7fffd18d2850_0, 0, 32;
    %delay 5, 0;
    %end;
    .scope S_0x7fffd18bed90;
T_1 ;
    %wait E_0x7fffd18bd440;
    %load/vec4 v0x7fffd18abd20_0;
    %load/vec4 v0x7fffd18d2330_0;
    %sub;
    %store/vec4 v0x7fffd18d2410_0, 0, 32;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x7fffd18beb60;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd18beb60 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd18d2760_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd18d2850_0, 0, 32;
    %pushi/vec4 10, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %fork TD_tb_sub.stimulus, S_0x7fffd18d2580;
    %join;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %end;
    .thread T_2;
    .scope S_0x7fffd18beb60;
T_3 ;
    %delay 500, 0;
    %vpi_call 2 42 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_sub.sv";
    "sub.sv";
