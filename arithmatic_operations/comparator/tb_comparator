    %delay 30, 0;
    %movi 8, 1, 4;
    %set/v v0112E470_0, 8, 4;
    %movi 8, 2, 4;
    %set/v v0112E4C8_0, 8, 4;
    %delay 30, 0;
    %movi 8, 1, 4;
    %set/v v0112E470_0, 8, 4;
    %movi 8, 3, 4;
    %set/v v0112E4C8_0, 8, 4;
    %delay 30, 0;
    %movi 8, 3, 4;
    %set/v v0112E470_0, 8, 4;
    %movi 8, 3, 4;
    %set/v v0112E4C8_0, 8, 4;
    %delay 30, 0;
    %movi 8, 7, 4;
    %set/v v0112E470_0, 8, 4;
    %movi 8, 7, 4;
    %set/v v0112E4C8_0, 8, 4;
